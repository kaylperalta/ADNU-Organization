<!DOCTYPE html>
<html>
<head>
  <title>Organizations</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if !user_signed_in? %>
		<p class = "sign_in"> <%= link_to "Admin Login", new_user_session_path(@Organizations) %></p>
		<% end %>

	<% if user_signed_in? %>
<div class="buttons">
<buttons class="buttons"> <%= link_to "New Post", new_user_session_path %></buttons>
<buttons class="buttons"> <%= link_to('Log Out', destroy_user_session_path, :method => :delete) %></buttons>
</div>
<% end %>
		
		
			<p class="notice"><%= notice %></p>
       	    <p class="alert"><%= alert %></p>	
<%= yield %>

</body>
</html>
